<div class="side-bar-header">
  <div class="header-name2">
  <%=@user.name%>
  <%= render "layouts/level" %>
  <div><%=@user.current_sign_in_at.strftime('%m月%d日 %H:%M')%></div>
  </div>
  <div>
  <div class="back-from-diary"><%=link_to "時間", diary_path(@user.diary_ids) %></div>
  <div class="back-from-diary"><%=link_to "back", root_path %></div>
  </div>
</div>

<div class="rooms">
  <% @diaries.each do |diary| %>
    <div class="room2">
      <div class="room-name">
      <%=link_to new_diary_reply_path(diary) do %>
         <div class="diary-title2"><%=diary.title %></div>
         <div><%=diary.created_at.strftime('%m月%d日 %H:%M') %></div>
         <% end %>
      </div>
    </div>
    <%end%>
  <div class="room-members">  

  </div>
</div>
